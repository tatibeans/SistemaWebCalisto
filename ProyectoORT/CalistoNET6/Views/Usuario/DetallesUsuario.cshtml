@model CalistoNET6.Models.ModeloUsuario


<div class="modal-header">
    <h3 class="align-middle">Detalles del usuario</h3>
     <button type="button" class="btn-close align-middle" data-bs-dismiss="modal" aria-label="Close"></button>
  
</div>
<div class="modal-body">

    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Correo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Correo)
        </dd>
          
        <dt>
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Telefono)      
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Tipos)
        </dt>

        <dd>
            @ViewBag.TipoUsu
        </dd>


    </dl>
</div>

